local library = {}

function library:Main(name, version)
    local ScreenGui = Instance.new("ScreenGui")
    local TopBar = Instance.new("Frame")
    local Container = Instance.new("Frame")
    local Tabs = Instance.new("Frame")
    local Name = Instance.new("TextLabel")
    local Version = Instance.new("TextLabel")

    ScreenGui.Parent = game.CoreGui
    ScreenGui.ResetOnSpawn = false

    TopBar.Name = "TopBar"
    TopBar.Parent = ScreenGui
    TopBar.BackgroundColor3 = Color3.fromRGB(12, 12, 12)
    TopBar.BorderSizePixel = 0
    TopBar.Position = UDim2.new(0.29831934, 0, 0.329561532, 0)
    TopBar.Size = UDim2.new(0, 511, 0, 25)
    TopBar.Active = true
    TopBar.Draggable = true

    Container.Name = "Container"
    Container.Parent = TopBar
    Container.BackgroundColor3 = Color3.fromRGB(24, 24, 24)
    Container.BorderSizePixel = 0
    Container.Position = UDim2.new(0, 0, 1, 0)
    Container.Size = UDim2.new(0, 511, 0, 261)

    Tabs.Name = "Tabs"
    Tabs.Parent = Container
    Tabs.BackgroundColor3 = Color3.fromRGB(12, 12, 12)
    Tabs.BorderSizePixel = 0
    Tabs.Size = UDim2.new(0, 117, 0, 261)

    

    Name.Name = "Name"
    Name.Parent = TopBar
    Name.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Name.BackgroundTransparency = 1.000
    Name.Position = UDim2.new(0.0156555772, 0, 0, 0)
    Name.Size = UDim2.new(0, 80, 0, 25)
    Name.Font = Enum.Font.SourceSansLight
    Name.Text = name
    Name.TextColor3 = Color3.fromRGB(255, 255, 255)
    Name.TextSize = 18.000
    Name.TextXAlignment = Enum.TextXAlignment.Left

    Version.Name = "Version"
    Version.Parent = TopBar
    Version.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Version.BackgroundTransparency = 1.000
    Version.Position = UDim2.new(0.172211364, 0, 0, 0)
    Version.Size = UDim2.new(0, 53, 0, 25)
    Version.Font = Enum.Font.SourceSansBold
    Version.Text = version
    Version.TextColor3 = Color3.fromRGB(255, 124, 227)
    Version.TextSize = 20.000
    Version.TextXAlignment = Enum.TextXAlignment.Left

    local Sections = Instance.new("Folder")
    Sections.Parent = Container

    local UIGridLayout = Instance.new("UIGridLayout")

    UIGridLayout.Parent = Tabs
    UIGridLayout.SortOrder = Enum.SortOrder.LayoutOrder
    UIGridLayout.CellSize = UDim2.new(0, 117, 0, 25)

    local section = {}

    function section:Section(text)
        local Features = Instance.new("Frame")

        local UIGridLayout = Instance.new("UIGridLayout")
        UIGridLayout.Parent = Features
        UIGridLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
        UIGridLayout.SortOrder = Enum.SortOrder.LayoutOrder
        UIGridLayout.CellPadding = UDim2.new(0, 5, 0, 10)
        UIGridLayout.CellSize = UDim2.new(0, 375, 0, 27)

        Features.Name = text
        Features.Parent = Sections
        Features.BackgroundColor3 = Color3.fromRGB(12, 12, 12)
        Features.BackgroundTransparency = 1.000
        Features.BorderSizePixel = 0
        Features.Position = UDim2.new(0.228962824, 0, 0.0306513403, 0)
        Features.Size = UDim2.new(0, 394, 0, 242)
        Features.Visible = false

        local SectionButton = Instance.new("TextButton")
        
        SectionButton.Parent = Tabs
        SectionButton.BackgroundColor3 = Color3.fromRGB(24, 24, 24)
        SectionButton.BorderSizePixel = 0
        SectionButton.Size = UDim2.new(0, 117,0, 24)
        SectionButton.Font = Enum.Font.SourceSansLight
        SectionButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        SectionButton.TextSize = 17.000
        SectionButton.Text = text
        SectionButton.AutoButtonColor = false
        SectionButton.MouseButton1Down:Connect(function()
            for i, v in next, Sections:GetChildren() do
                if v:IsA("Frame") then
                    v.Visible = false
                end
            end
            Features.Visible = true
        end)


        local other = {}

        function other:Button(text, callback)
            local callback = callback or function() end
    
            local ButtonFrame = Instance.new("ImageLabel")
            local Button = Instance.new("TextButton")
            local ButtonUnderline = Instance.new("Frame")
    
            
            ButtonFrame.Name = "Button"
            ButtonFrame.Parent = Features
            ButtonFrame.BackgroundColor3 = Color3.fromRGB(12, 12, 12)
            ButtonFrame.BackgroundTransparency = 1.000
            ButtonFrame.Size = UDim2.new(0, 100, 0, 100)
            ButtonFrame.Image = "rbxassetid://3570695787"
            ButtonFrame.ImageColor3 = Color3.fromRGB(12, 12, 12)
            ButtonFrame.ScaleType = Enum.ScaleType.Slice
            ButtonFrame.SliceCenter = Rect.new(100, 100, 100, 100)
            ButtonFrame.SliceScale = 0.030
    
            Button.Name = "Button"
            Button.Parent = ButtonFrame
            Button.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Button.BackgroundTransparency = 1.000
            Button.BorderSizePixel = 0
            Button.Size = UDim2.new(0, 375, 0, 25)
            Button.Font = Enum.Font.SourceSansLight
            Button.Text = text
            Button.TextColor3 = Color3.fromRGB(255, 255, 255)
            Button.TextSize = 23.000
    
            ButtonUnderline.Parent = ButtonFrame
            ButtonUnderline.BackgroundColor3 = Color3.fromRGB(255, 124, 227)
            ButtonUnderline.BorderSizePixel = 0
            ButtonUnderline.Position = UDim2.new(0, 0, 0.92592591, 0)
            ButtonUnderline.Size = UDim2.new(0, 375, 0, 1)
    
            Button.MouseButton1Down:Connect(function()
                pcall(callback)
            end)
        end

        function other:Slider(text, minvalue, maxvalue, callback, data)
            local callback = callback or function() end;
    
    
    
            local Nubmer = Instance.new("TextLabel")
            local Slider = Instance.new("ImageLabel")
            local slider = Instance.new("ImageButton")
            local SliderText = Instance.new("TextLabel")
            local SliderInner = Instance.new("ImageLabel")
    
            Slider.Name = "Slider"
            Slider.Parent = Features
            Slider.BackgroundColor3 = Color3.fromRGB(12, 12, 12)
            Slider.BackgroundTransparency = 1.000
            Slider.Size = UDim2.new(0, 100, 0, 100)
            Slider.Image = "rbxassetid://3570695787"
            Slider.ImageColor3 = Color3.fromRGB(12, 12, 12)
            Slider.ScaleType = Enum.ScaleType.Slice
            Slider.SliceCenter = Rect.new(100, 100, 100, 100)
            Slider.SliceScale = 0.030
    
            slider.Name = "slider"
            slider.Parent = Slider
            slider.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            slider.BackgroundTransparency = 1.000
            slider.Position = UDim2.new(0.0209999997, 0, 0.600000024, 0)
            slider.Size = UDim2.new(0, 361, 0, 7)
            slider.Image = "rbxassetid://3570695787"
            slider.ScaleType = Enum.ScaleType.Slice
            slider.SliceCenter = Rect.new(100, 100, 100, 100)
            slider.SliceScale = 0.020
    
            SliderText.Parent = slider
            SliderText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            SliderText.BackgroundTransparency = 1.000
            SliderText.Position = UDim2.new(-0.00072396826, 0, -2.31428742, 0)
            SliderText.Size = UDim2.new(0, 200, 0, 11)
            SliderText.Font = Enum.Font.SourceSansLight
            SliderText.Text = text
            SliderText.TextColor3 = Color3.fromRGB(255, 255, 255)
            SliderText.TextSize = 18.000
            SliderText.TextXAlignment = Enum.TextXAlignment.Left
    
            SliderInner.Name = "SliderInner"
            SliderInner.Parent = slider
            SliderInner.BackgroundColor3 = Color3.fromRGB(255, 124, 227)
            SliderInner.BackgroundTransparency = 1.000
            SliderInner.Size = UDim2.new(0, 0, 0, 7)
            SliderInner.Image = "rbxassetid://3570695787"
            SliderInner.ImageColor3 = Color3.fromRGB(255, 124, 227)
            SliderInner.ScaleType = Enum.ScaleType.Slice
            SliderInner.SliceCenter = Rect.new(100, 100, 100, 100)
            SliderInner.SliceScale = 0.020
            SliderInner.Position = UDim2.new(0, 0,0, 0)
            local Value
            local mouse = game.Players.LocalPlayer:GetMouse()
            local uis = game:GetService("UserInputService")
            Nubmer.Name = "Nubmer"
            Nubmer.Parent = slider
            Nubmer.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Nubmer.BackgroundTransparency = 1.000
            Nubmer.Position = UDim2.new(0.238227144, 0, -2.31428742, 0)
            Nubmer.Size = UDim2.new(0, 70, 0, 16)
            Nubmer.Font = Enum.Font.SourceSansBold
            Nubmer.TextColor3 = Color3.fromRGB(255, 255, 255)
            Nubmer.TextSize = 12.000
            Nubmer.Text = "0"
            
            
            slider.MouseButton1Down:Connect(function()
                Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 360) * SliderInner.AbsoluteSize.X) + tonumber(minvalue))
                pcall(function()
                    callback(Value)
                end)
                SliderInner.Size = UDim2.new(0, math.clamp(mouse.X - SliderInner.AbsolutePosition.X, 0, 360), 0, 7)
                moveconnection = mouse.Move:Connect(function()
                    Nubmer.Text = Value
                    Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 360) * SliderInner.AbsoluteSize.X) + tonumber(minvalue))
                    pcall(function()
                        callback(Value)
                    end)
                    SliderInner.Size = UDim2.new(0, math.clamp(mouse.X - SliderInner.AbsolutePosition.X, 0, 360), 0, 7)
                end)
                releaseconnection = uis.InputEnded:Connect(function(Mouse)
                    if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then
                        Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 360) * SliderInner.AbsoluteSize.X) + tonumber(minvalue))
                        pcall(function()
                            callback(Value)
                        end)
                        SliderInner.Size = UDim2.new(0, math.clamp(mouse.X - SliderInner.AbsolutePosition.X, 0, 360), 0, 7)
                        moveconnection:Disconnect()
                        releaseconnection:Disconnect()
                    end
                end)
            end)
        end

        function other:Toggle(text, callback)
            local callback = callback or function() end;
    
            local Toggle = Instance.new("ImageLabel")
            local ToggleText = Instance.new("TextLabel")
            local ToggleOff = Instance.new("ImageButton")
            local ToggleOn = Instance.new("ImageButton")
    
            Toggle.Name = "Toggle"
            Toggle.Parent = Features
            Toggle.BackgroundColor3 = Color3.fromRGB(12, 12, 12)
            Toggle.BackgroundTransparency = 1.000
            Toggle.Size = UDim2.new(0, 100, 0, 100)
            Toggle.Image = "rbxassetid://3570695787"
            Toggle.ImageColor3 = Color3.fromRGB(12, 12, 12)
            Toggle.ScaleType = Enum.ScaleType.Slice
            Toggle.SliceCenter = Rect.new(100, 100, 100, 100)
            Toggle.SliceScale = 0.030
    
            ToggleText.Name = "ToggleText"
            ToggleText.Parent = Toggle
            ToggleText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ToggleText.BackgroundTransparency = 1.000
            ToggleText.Position = UDim2.new(0.0213333331, 0, 0, 0)
            ToggleText.Size = UDim2.new(0, 311, 0, 27)
            ToggleText.Font = Enum.Font.SourceSansLight
            ToggleText.Text = text
            ToggleText.TextColor3 = Color3.fromRGB(255, 255, 255)
            ToggleText.TextSize = 19.000
            ToggleText.TextXAlignment = Enum.TextXAlignment.Left
    
            ToggleOff.Name = "ToggleOff"
            ToggleOff.Parent = Toggle
            ToggleOff.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ToggleOff.BackgroundTransparency = 1.000
            ToggleOff.Position = UDim2.new(0.885333359, 0, 0.148148149, 0)
            ToggleOff.Size = UDim2.new(0, 37, 0, 18)
            ToggleOff.Image = "rbxassetid://3570695787"
            ToggleOff.ImageColor3 = Color3.fromRGB(255, 51, 33)
            ToggleOff.ScaleType = Enum.ScaleType.Slice
            ToggleOff.SliceCenter = Rect.new(100, 100, 100, 100)
            ToggleOff.SliceScale = 0.050
    
            ToggleOn.Name = "ToggleOn"
            ToggleOn.Parent = Toggle
            ToggleOn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ToggleOn.BackgroundTransparency = 1.000
            ToggleOn.Position = UDim2.new(0.885333359, 0, 0.148148149, 0)
            ToggleOn.Size = UDim2.new(0, 0,0, 18)
            ToggleOn.Image = "rbxassetid://3570695787"
            ToggleOn.ImageColor3 = Color3.fromRGB(19, 255, 105)
            ToggleOn.ScaleType = Enum.ScaleType.Slice
            ToggleOn.SliceCenter = Rect.new(100, 100, 100, 100)
            ToggleOn.SliceScale = 0.050
    
            local function Fire()
                enabled = not enabled
                ToggleOn:TweenSize(enabled and UDim2.new(0, 37, 0, 18) or UDim2.new(0, 0, 0, 18), "InOut", "Linear", 0)
                pcall(callback, enabled)
            end
    
            ToggleOn.MouseButton1Click:Connect(Fire)
            ToggleOff.MouseButton1Click:Connect(Fire)
        end
        return other;
    end
    return section;
end
return library
